<html>
	<head>
		<title>Users</title>
	</head>
	<body>
		<table border=1>
			<tr>
				<th>ID</th>
				<th>User Name</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Password</th>
				<th>Email</th>
				<th>Date of Birth</th>
			</tr>
			{{#each user}}
				<tr>
					<th>{{this.id}}</th>
					<th>{{this.user_name}}</th>
					<th>{{this.first_name}}</th>
					<th>{{this.last_name}}</th>
					<th>{{this.password}}</th>
					<th>{{this.email}}</th>
					<th>{{this.date_of_birth}}</th>
				</tr>
			{{/each}}
		</table>
	</body>
</html>